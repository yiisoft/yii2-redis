{
    "name": "yiisoft/yii2-redis",
    "description": "Redis Cache, Session and ActiveRecord for the Yii framework",
    "keywords": ["yii2", "redis", "active-record", "cache", "session"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-redis/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2-redis"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-openssl": "*",
        "yiisoft/yii2": "~2.0.50",
        "predis/predis": "^v2.3.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "yiisoft/yii2-dev": "~2.0.50"
    },
    "autoload": {
        "psr-4": { "yii\\redis\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\redis\\": "tests/",
            "yiiunit\\extensions\\predis\\": "tests/predis/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
