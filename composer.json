{
    "name": "yiisoft/yii2-redis",
    "description": "Redis Cache, Session and ActiveRecord for the Yii framework",
    "keywords": [
        "yii2",
        "redis",
        "active-record",
        "cache",
        "session"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-redis/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2-redis"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-openssl": "*",
        "yiisoft/yii2": "22.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": { "yii\\redis\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "yiiunit\\extensions\\redis\\": "tests/"}
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
